{
  "name": "kiyotd/utils",
  "description": "PHP Utilities `kiyotd/utils` for PHP 7.4 and above",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/kiyotd/utils-php",
  "license": "MIT",
  "authors": [
    {
      "name": "kiyotd"
    }
  ],
  "support": {
    "issues": "https://github.com/kiyotd/utils-php/issues"
  },
  "prefer-stable": true,
  "require": {
    "php": "^8.1"
  },
  "config": {
    "platform": {
      "php": "8.1.24"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "kiyotd\\utils\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.2-dev"
    }
  }
}
